{
  "name": "InheritNext_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create InheritNext_backend && dfx generate InheritNext_backend && dfx deploy",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "prebuild": "npm run generate",
    "generate": "mkdir -p src/icp-declarations/InheritNext_backend && icp-bindgen --did-file ../../src/InheritNext_backend/InheritNext_backend.did --out-dir src/icp-declarations/InheritNext_backend --force"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.9",
    "@dfinity/agent": "^3.0.0",
    "@dfinity/auth-client": "^3.4.3",
    "@dfinity/candid": "^3.0.0",
    "@dfinity/identity": "^3.4.3",
    "@dfinity/principal": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@icp-sdk/auth": "^5.0.0",
    "@icp-sdk/canisters": "^3.4.0",
    "@icp-sdk/core": "^5.0.0",
    "framer-motion": "^12.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@icp-sdk/bindgen": "^0.2.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "dotenv": "^16.3.1",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3"
  }
}
